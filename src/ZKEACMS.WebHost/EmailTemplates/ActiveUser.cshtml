@model Easy.Modules.User.Models.UserEntity
<!DOCTYPE html>
<html>
<head>
    <title>
        Activate user

    </title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
</head>
<body>
    <h3>
        Activate user


    </h3>
    <p>
      


        @if (Model.Status == 1)
        {
             <div>   Your account has been activated.</div> 

        }
        else 
        {  
            <div> The registration information is incomplete or inaccurate, please check it again</div>
        }

    </p>
  
  
    <p>
        <em>@DateTime.Now.ToString("yyyy-MM-dd")</em>
    </p>
</body>
</html>